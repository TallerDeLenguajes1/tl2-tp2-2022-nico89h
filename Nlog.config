<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<targets>
		<target name="logfile" xsi:type="File" fileName="file.txt"/>
		<target name="logconsole" xsi:type="Console" />
		<target name="Otros" xsi:type="File" fileName="Applog.log"/>
		<target name="Info" xsi:type="File" fileName="Infolog.log"/>
	</targets>

	<rules>
		<logger name="*" minlevel="Info" writeTo="Info"/>
		<logger name="*" minlevel="Debug" writeTo="logconsole" />
		<logger name="*" minlevel="Error" writeTo="Otros"/>
		<logger name="*" minlevel="Warn" writeTo="Otros"/>
		<logger name="*" minlevel="Fatal" writeTo="Otros"/>
		<logger name="*" minlevel="Trace" writeTo="Otros"/>
	</rules>
</nlog>